{
	"title": "Crawlab文档",
	"plugins": [
		"expandable-chapters",
		"chapter-fold",
		"search-pro",
		"back-to-top-button",
		"splitter",
		"github",
		"edit-link",
		"favicon",
		"lightbox",
		"prism", 
		"-highlight",
		"hide-element",
		"tbfed-pagefooter",
		"addcssjs",
		"sitemap"
	],
	"pluginsConfig": {
		"github": {
			"url": "https://github.com/crawlab-team/crawlab"
		},
		"edit-link": {
			"base": "//github.com/crawlab-team/crawlab-docs/edit/master",
			"label": "编辑此页面"
		},
		"favicon": {
			"shortcut": "assets/images/favicon.ico"
		},
		"hide-element": {
			"elements": [".gitbook-link"]
		},
		"tbfed-pagefooter": {
			"copyright":"© 2020 Crawlab, Made by Crawlab-Team",
			"modify_label": "该文件最后修改时间: ",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"addcssjs": {
			"js": [
				"assets/js/track.js"
			],
			"css": []
		},
		"sitemap": {
            "hostname": "http://docs.crawlab.cn/"
        }
	},
	"styles": {
		"website": "assets/css/website.css"
	}
}